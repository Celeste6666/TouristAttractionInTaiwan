{
  "name": "touristattractionintaiwan",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.0",
    "core-js": "^3.6.5",
    "crypto-js": "^4.1.1",
    "jssha": "^3.2.0",
    "swiper": "^6.8.4",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^7.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/base",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "max-len": [
        "warn",
        {
          "code": 200,
          "ignoreUrls": true
        }
      ],
      "import/extensions": [
        "warn",
        "ignorePackages"
      ],
      "object-curly-newline": [
        "warn",
        {
          "ObjectExpression": {
            "multiline": true
          },
          "ObjectPattern": {
            "multiline": true
          },
          "ImportDeclaration": {
            "multiline": true
          },
          "ExportDeclaration": {
            "multiline": true,
            "minProperties": 5
          }
        }
      ],
      "comma-dangle": [
        "error",
        "only-multiline"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
